[mysqld]
ssl-ca=/bitnami/cert/ca-cert.pem
ssl-cert=/bitnami/cert/server-cert.pem
ssl-key=/bitnami/cert/server-key.pem

[client]
ssl-ca=/bitnami/cert/ca-cert.pem
ssl-cert=/bitnami/cert/client-cert.pem
ssl-key=/bitnami/cert/client-key.pem

[sst]
encrypt=4
tca = /bitnami/cert/ca-cert.pem
tkey = /bitnami/cert/server-key.pem
tcert = /bitnami/cert/server-cert.pem